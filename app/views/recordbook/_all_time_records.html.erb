<div class="typewriter home-header text-3xl px-4 py-4">
  <span class="underline-blue">Wins</span>
</div>
<table class="table flex mb-6 shadow">
  <% @wins.each do |value| %>
    <tr class="typewriter bg-indigo-50 border-b-2 border-indigo-500">
      <td class="px-6 py-2"><%= value[1] %></td>
      <td class="px-6 py-2"><%= Member.find(value[0]).full_name %></td>
    </tr>
  <% end %>
</table>

